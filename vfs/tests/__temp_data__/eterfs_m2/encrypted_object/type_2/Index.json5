{
  // Search Mode can be either "Disk" or "Virtual", default mode is "Virtual"
  // When getting files it will first search on the "Disk" or in the
  "search_mode": "Disk",

  // Here is a registry of the pack files and their virtual paths
  // Pack files content are related to their virtual space, for example
  // the "*" is a wildcard that means "any pack file" and "pack/" means that
  // "any pack file" has the "pack/" path
  "packs": {
    "*": [
      "pack/"
    ],

    "pc": [
      "d:/ymir work/pc/"
    ],

    "pc2": [
      "d:/ymir work/pc2"
    ],

    "monster": [
      "d:/ymir work/monster/monster"
    ],

    "monster2": [
      "d:/ymir work/monster2/"
    ],

    "npc": [
      "d:/ymir work/npc/"
    ],

    "npc2": [
      "d:/ymir work/npc2/"
    ],

    "item": [
      "d:/ymir work/item/"
    ],

    "icon": [
      "icon/"
    ],

    "etc": [
      "d/:ymir work/etc/",
      "d:/ymir work/special/",
      "d:/ymir work/environment/",
      "d:/ymir work/ui/"
    ],

    "sound": [
      "sound/ambience/",
      "sound/common/",
      "sound/effect/",
      "sound/pc/",
      "sound/npc/",
      "sound/monster/",
      "sound/ui/",
    ],

    "bgm": [
      "bgm/"
    ],

    "effect": [
      "d:/ymir work/effect/"
    ],

    "textureset": [
      "textureset/"
    ],

    "property": [
      "property/"
    ],

    "terrain": [
      "d:/ymir work/terrainmaps/"
    ],

    "zone": [
      "d:/ymir work/zone/"
    ],

    "tree": [
      "d:/ymir work/tree/"
    ],

    "guild": [
      "d:/ymir work/guild/"
    ],

    "locale_en": [
      "locale/en/"
    ],

    "uiscript": [
      "uiscript/"
    ],

    "uiloading": [
      "d:/ymir work/uiloading/"
    ],

    "metin2_map_a1": [
      "metin2_map_a1/",

    ]
  },
}

